class formulaProg:
    def __init__(self):
        pass

    def main(self):
        print(" Enter Config Type >>>>n 1.FixedBias, 2.Emitter Bias  ")
        exe = formulaProg()
        type = int(input())
        if type == 1:
            exe.fixedBias()
        elif type == 2:
            exe.emitterbias()

    def fixedBias(self):
        print("Enter Values For....")
        print("Enter Voltage CC >>> ")
        Vcc = int(input())
        print("Enter Voltage BE >>> ")
        Vbe = int(input())
        print("Enter Rb>>> ")
        Rb = int(input())
        print("Enter Rc>>> ")
        Rc = int(input())
        print("Enter Beta >>> ")
        Beta = int(input())
        Ib = (Vcc - Vbe) / Rb
        Ic = (Beta + 1) * Ib
        Vce = Vcc - (Ic * Rc)
        print("Value of Ib...." + str(Ib))
        print("Value of Ic...." + str(Ic))
        print("Value of Vce...." + str(Vce))

    def emitterbias(self):
        print("Enter Values For....")
        print("Enter Voltage CC >>> ")
        Vcc = int(input())